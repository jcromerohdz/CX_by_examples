package main

func plus(a i32, b i32) (out i32){
  out = i32.add(a, b)
}

func plusPlus(a i32, b i32, c i32) (out i32){
  out = i32.add(a, b) 
  out = i32.add(out, c)
}

func main()(){
  str.print("Simple plus funciton in CX")
  i32.print(plus(5, 5))
  str.print("Simple plusPlus funciton in CX")
  i32.print(plusPlus(5, 5, 5))
}
